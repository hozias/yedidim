@model IEnumerable<YedideyChabad.Models.Yadid>

@{
    ViewBag.Title = "yedidim main";
    Layout = "~/Views/Shared/_Layout-Loggedin.cshtml";
}

<div class="start-page-options">
    <ul>
        <li>
            <div class="click-active">
                <a href='@Url.Action("GetAll", "Start")' class="ico-font-con">
                    <div class="icon-group"></div>
                    <div class="text">ידידים</div>
                    <div class="sub-text">צפה, הוסף וערוך ידידים</div>
                </a>
            </div>
        </li>
        <li class="disabled-item">
            <div class="click-active">
                <a href='' onclick="return false;" class="ico-font-con">
                    <div class="icon-action"></div>
                    <div class="text">פעילות</div>
                    <div class="sub-text">שיעורים, פגישות ופעילויות נוספות</div>
                </a>
            </div>
        </li>
        <li class="disabled-item">
            <div class="click-active">
                <a href='' onclick="return false;" class="ico-font-con">
                    <div class="icon-calendar"></div>
                    <div class="text">השבוע</div>
                    <div class="sub-text">אירועים, ימי הולדת ותאריכים נוספים</div>
                </a>
            </div>
        </li>
    </ul>
